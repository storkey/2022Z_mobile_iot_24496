<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RealEstateApp.Pages.CustomTabbedPage"
             xmlns:local="clr-namespace:RealEstateApp.Pages"
             xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
             android:TabbedPage.ToolbarPlacement="Bottom"
             SelectedTabColor="DarkSeaGreen"
             UnselectedTabColor="Black">

    <NavigationPage Title="Główna" IconImageSource="home_icon" BackgroundColor="DarkOliveGreen">
        <x:Arguments>
            <local:HomePage/>
        </x:Arguments>
    </NavigationPage>
    <local:BookmarksPage Title="Zapisane" IconImageSource="bookmark_icon" />
</TabbedPage>